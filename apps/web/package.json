{
  "name": "@sde/web",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "pnpm --silent with-env next dev",
    "build": "pnpm --silent with-env next build",
    "postbuild": "next-sitemap",
    "modclean": "modclean -Pr",
    "modclean:root": "modclean -Pr -p ./../..",
    "start": "next start",
    "lint": "eslint src && ../../packages/lint/node_modules/.bin/stylelint \"src/**/*.css\"",
    "lint:eslint": "eslint src",
    "lint:style": "../../packages/lint/node_modules/.bin/stylelint \"src/**/*.css\"",
    "tsc": "tsc --noEmit",
    "postinstall": "pnpm --silent setup:dsfr && pnpm --silent prisma:generate",
    "setup:dsfr": "FROM=\"./node_modules/@gouvfr/dsfr/dist\"; TO=\"public/dsfr\"; if [ ! -d \"$FROM\" ]; then FROM=\"../../node_modules/@gouvfr/dsfr/dist\"; fi; mkdir -p public/dsfr/utility && cp $FROM/dsfr.module.min.js $TO/ && cp $FROM/dsfr.module.min.js.map $TO/ && cp $FROM/dsfr.nomodule.min.js $TO/ && cp $FROM/dsfr.nomodule.min.js.map $TO/ && cp $FROM/dsfr.min.css $TO/ && cp $FROM/utility/utility.min.css $TO/utility/ && cp -R $FROM/icons $TO/ && cp -R $FROM/artwork $TO/ && cp -R $FROM/fonts $TO/",
    "test": "jest",
    "with-env": "dotenv -e ../../.env --",
    "with-remote-env": "dotenv -e ../../.env.remote -e ../../.env --",
    "prisma:dev": "pnpm --silent with-env prisma studio --port 5556",
    "prisma:db:push": "pnpm --silent with-env prisma db push --skip-generate",
    "prisma:remote:db:push": "pnpm --silent with-remote-env prisma db push --skip-generate",
    "prisma:migrate:dev": "pnpm --silent with-env prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:pull": "pnpm --silent with-env prisma db pull",
    "prisma": "pnpm --silent with-env prisma"
  },
  "engines": {
    "node": "^18.16"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.438.0",
    "@aws-sdk/s3-request-presigner": "^3.438.0",
    "@gouvfr/dsfr": "^1.10.2",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@react-spring/web": "^9.7.3",
    "@sde/emails": "*",
    "@sde/storybook": "*",
    "@sentry/browser": "^7.77.0",
    "@sentry/nextjs": "^7.77.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.0",
    "@trpc/react-query": "^10.43.0",
    "@trpc/server": "^10.43.0",
    "axios": "^1.3.4",
    "chart.js": "^4.4.0",
    "client-only": "^0.0.1",
    "copy": "^0.3.2",
    "csv-stringify": "^6.4.4",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "encoding": "^0.1.13",
    "env-var": "^7.4.1",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.0",
    "fuse.js": "^7.0.0",
    "js-cookie": "^3.0.5",
    "js-file-downloader": "^1.1.25",
    "maplibre-gl": "^3.5.2",
    "nanoid": "^3.3.6",
    "next": "^14.0.3-canary.6",
    "next-auth": "^4.24.4",
    "next-sitemap": "^4.2.3",
    "node-html-parser": "^6.1.11",
    "nodemailer": "^6.9.7",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-intersection-observer": "^9.5.2",
    "react-loader-spinner": "^5.4.5",
    "react-swipeable": "^7.0.1",
    "react-textarea-autosize": "^8.5.3",
    "react-use-css-custom-property": "^0.1.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "smooth-scroll-into-view-if-needed": "^2.0.2",
    "superjson": "^2.2.0",
    "uglify-js": "^3.17.4",
    "undici": "^5.27.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.2",
    "@sde/test": "*",
    "@types/file-saver": "^2.0.6",
    "@types/js-cookie": "^3.0.5",
    "@types/mime-types": "^2.1.3",
    "@types/node": "^18.16.13",
    "@types/nodemailer": "^6.4.13",
    "@types/nprogress": "^0.2.2",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.6",
    "css-loader": "^6.8.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "joi": "^17.9.1",
    "modclean": "^3.0.0-beta.1",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.2.0",
    "react-is": "^18.2.0",
    "require-from-string": "^2.0.2",
    "style-loader": "^3.3.3",
    "typescript": "^5.0.3"
  }
}
